{
  "name": "task-management-mini-ap",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -b",
    "serve": "node dist/backend/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "prisma generate --schema=src/backend/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/backend/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/backend/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/backend/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=src/backend/prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema=src/backend/prisma/schema.prisma",
    "db:seed": "ts-node src/backend/prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
